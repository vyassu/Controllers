{
  "CFx": {
    "Model": "GroupVPN"
  },
  "Logger": {
    "Enabled": true,
    "LogLevel": "INFO",
    "LogOption": "Console",
    "BackupLogFileCount": 5,
    "LogFileName": "controller.log",
    "LogFileSize": 10000
  },
  "VirtualNetworkInitializer": {
    "Enabled": true,
    "LogLevel": "WARNING",
    "Vnets": [
      {
        "IP4": "10.10.240.101",
        "IP4Prefix": 20,
        "Mtu4" :  1200,
        "XMPPModuleName": "XmppClient",
        "TapName": "ipop_tap0",
        "Description": "Internal Beta 1 Test Network",
        "IgnoredNetInterfaces": [ "ipop_tap0" ],
        "L2TunnellingEnabled": 1

      }
    ],
    "Stun": [ "stun.l.google.com:19302" ],
    "Turn": [
      {
        "Address": "turn.ipop-project.org:19302",
        "User": "",
        "Password": ""
      }
    ],
    "dependencies": [ "Logger", "TincanInterface" ]
  },
  "XmppClient": {
    "Enabled": true,
    "Username": "@xmpp.ipop-project.org",
    "Password": "",
    "AddressHost": "xmpp.ipop-project.org",
    "Port": "5222",
    "TapName": "ipop_tap0",
    "AuthenticationMethod": "password",
    "AcceptUntrustedServer": true,
    "TimerInterval": 15,
    "dependencies": [ "Logger" ]
  },
  "BaseTopologyManager": {
    "Enabled": true,
    "NumberOfSuccessors": 100,
    "NumberOfChords": 0,
    "NumberOfOnDemand": 0,
    "NumberOfInbound": 20,
    "InitialLinkTTL": 120,
    "LinkPulse": 180,
    "OnDemandLinkTTL": 60,
    "TimerInterval": 1,
    "TopologyRefreshInterval": 15,
    "OndemandThreshold":1000,
    "OndemandConnectionWaitTime": 15,
    "NumberOfPingsToPeer": 5,
    "PeerPingInterval": 300,
    "MaxConnRetry": 5,
    "dependencies": [ "Logger" ]
  },
  "TincanInterface": {
    "Enabled": true,
    "SocketReadWaitTime": 15,
    "dependencies": [ "Logger" ]
  },
  "OverlayVisualizer": {
    "Enabled": true,
    "WebServiceAddress": "ms1020.utah.cloudlab.us:8080/insertdata",
    "TopologyDataQueryInterval": 5,
    "WebServiceDataPostInterval": 5,
    "TimerInterval": 5,
    "NodeName": "101",
    "dependencies": [ "Logger" ]
  },
  "BroadCastForwarder": {
    "Enabled": true,
    "dependencies": [ "Logger" ]
  },
  "NodeDiscovery": {
    "Enabled": true,
    "OnDemandThreshold": 15,
    "dependencies": [ "Logger" ]
  },
  "ConnectionManager": {
    "Enabled": true,
    "InitialLinkTTL": 120,
    "ChordLinkTTL": 180,
    "OndemandLinkRateThreshold": 128,
    "dependencies": [ "Logger" ]
  },
  "IPMulticast": {
    "Enabled": true,
    "dependencies": ["Logger"]
  },
  "OverlayMulticast": {
    "Enabled": true,
    "dependencies": ["Logger"]
  }
}
